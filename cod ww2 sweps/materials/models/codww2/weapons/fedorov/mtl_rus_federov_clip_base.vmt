VertexLitGeneric
{
	"$basetexture" "models\codww2\weapons\fedorov\clip"
	"$bumpmap" "models\codww2\weapons\fedorov\clip_n"
$phongexponenttexture "models/codww2/weapons/fedorov/clip_s"

$detail  "models\codww2\weapons\fedorov\clip_d"
$detailscale 1	
$detailblendfactor 0.05
$detailblendmode 2

"$phong"                      "1"
	"$phongboost"                 "0.15"
	"$phongfresnelranges"         "[5 10 10]"
	"$phongtint"		      "[1.05 1 1.15]"
//$phongexponent 100
	"$phongalbedotint"            "1.5"
	"$phongalbedoboost"           "200"
	"$halflambert"                "1"

$envmapfresnel 0.1
	"$envmap" "env_cubemap" // which envmap to use, "env_cubemap" can be used but this works best with static envmaps and the whole point of this is not to rely on env_cubemaps
	
	"Proxies" // needed proxy for it to work, put this in your material, chagnes the $envmaptint according to where the entity is positioned
	{
		"PB_ENVMAPTINT_REALTIME_COLOR"
		{
			"min" "0" // minimum brightness, should be 0, otherwise will be visible in pitch black areas
			"max" "0.075" // maximum brightness, sometimes small values like 0.25 or 0.5 are pretty much the max you need, depends on the material
		}
	}
	"$normalmapalphaenvmapmask"		"1"
}